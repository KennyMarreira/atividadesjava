import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int i, x;
		
		System.out.println("Digite um numero de 1 a 1000: ");
		x = sc.nextInt();
		
		for (i = 0; i < x; i++) {
			if (x % 2 != 0) {
				System.out.println(i);
			}
		}
		
		
		sc.close();
	}

}
